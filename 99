<insert id="insertOrUpdateMatch" parameterType="your.package.Match">
  INSERT INTO your_table (id, created_date, creator, update_date, updator, sport_type_id,
    sport_type_name, league_name, stream_source, match_date, match_time, top, league_id,
    match_id, home_name, away_name, match_status, home_id, away_id, live_home_score,
    live_away_score, is_live)
  VALUES (#{id}, #{createdDate}, #{creator}, #{updateDate}, #{updator}, #{sportTypeId},
    #{sportTypeName}, #{leagueName}, #{streamSource}, #{matchDate}, #{matchTime}, #{top},
    #{leagueId}, #{matchId}, #{homeName}, #{awayName}, #{matchStatus}, #{homeId},
    #{awayId}, #{liveHomeScore}, #{liveAwayScore}, #{isLive})
  ON DUPLICATE KEY UPDATE
    match_status = CASE
      WHEN #{matchStatus} <> match_status THEN #{matchStatus}
      ELSE match_status
    END,
    live_home_score = CASE
      WHEN #{liveHomeScore} <> live_home_score THEN #{liveHomeScore}
      ELSE live_home_score
    END,
    live_away_score = CASE
      WHEN #{liveAwayScore} <> live_away_score THEN #{liveAwayScore}
      ELSE live_away_score
    END,
    match_date = CASE
      WHEN #{matchDate} <> match_date THEN #{matchDate}
      ELSE match_date
    END,
    match_time = CASE
      WHEN #{matchTime} <> match_time THEN #{matchTime}
      ELSE match_time
    END
</insert>
